// SET FIELD NAME IN CONTEXT

// Returns FQFN based on current context

// Function Name : jsys.setFieldNameInContext
// Parameters : fieldName
// Use example : jsys.setFieldNameInContext ( "myField" )

Case ( PatternCount ( fieldName ; "::" ) ; fieldName ; Get ( LayoutTableName ) & "::" & fieldName )

